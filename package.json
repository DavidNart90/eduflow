{
  "name": "eduflow",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack",
    "dev:performance": "cross-env NODE_ENV=development ANALYZE=true next dev --turbopack",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "prepare": "husky",
    "clean:deps": "rm -rf node_modules package-lock.json && npm install",
    "clean:cache": "rm -rf .next out && npm run build",
    "performance:monitor": "node performance-monitor.js",
    "dev:cpu-optimized": "NODE_OPTIONS=\"--max-old-space-size=2048\" next dev --turbopack"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{json,md,css,scss}": [
      "prettier --write"
    ]
  },
  "dependencies": {
    "@heroicons/react": "^2.2.0",
    "@supabase/ssr": "^0.6.1",
    "@supabase/supabase-js": "^2.53.0",
    "clsx": "^2.1.1",
    "immer": "^10.1.3",
    "next": "^15.5.2",
    "paystack": "^2.0.1",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-loading-skeleton": "^3.5.0",
    "tailwind-merge": "^3.3.1",
    "zustand": "^5.0.7"
  },
  "optionalDependencies": {
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.1",
    "@mui/icons-material": "^7.3.1",
    "@mui/material": "^7.3.1",
    "@types/lodash": "^4.17.20",
    "@types/multer": "^2.0.0",
    "critters": "^0.0.23",
    "html2canvas": "^1.4.1",
    "jspdf": "^3.0.1",
    "jspdf-autotable": "^5.0.2",
    "jszip": "^3.10.1",
    "lodash": "^4.17.21",
    "material": "^0.9.24",
    "multer": "^2.0.2",
    "react-pdf": "^10.1.0",
    "ui": "^0.2.4",
    "xlsx": "^0.18.5"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@types/jszip": "^3.4.0",
    "@types/node": "^20.19.11",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "autoprefixer": "^10.4.21",
    "cross-env": "^10.0.0",
    "eslint": "^9",
    "eslint-config-next": "15.4.5",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.3",
    "husky": "^9.1.7",
    "lint-staged": "^16.1.2",
    "postcss": "^8.5.6",
    "prettier": "^3.6.2",
    "tailwind-scrollbar": "^3.1.0",
    "tailwindcss": "^3.4.17",
    "typescript": "^5"
  }
}
